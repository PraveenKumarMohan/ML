performanceSVM <- function(model, test, isTest) {
results <- predict(model, test)
if (isTest) {
cm <- confusionMatrix(results, mnist[-index, 1])
} else {
cm <- confusionMatrix(results, mnist[index, 1])
}
return(cm$overall[c(1:2)])
}
